{
  "compilerOptions": {
    "target": "ES2017",
    "module": "CommonJS",
    "moduleResolution": "Node",
    "strict": true,
    "strictPropertyInitialization": false,
    "declaration": true,
    "downlevelIteration": true,
    "allowSyntheticDefaultImports": true
  },
  "exclude": ["packages/**/lib"]
}
